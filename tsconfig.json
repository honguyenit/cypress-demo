{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es5", "dom"],
      "types": ["cypress", "node", "@types/cypress-cucumber-preprocessor"],
      "allowJs": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noEmit": true
    },
    "include": ["**/*.ts", "cypress/**/*.ts"],
    "exclude": ["node_modules"]
  }
  