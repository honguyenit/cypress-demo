{
  "name": "cypress-demo",
  "version": "1.0.0",
  "description": "test",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "test-headed": "npx cypress run --headed",
    "test-chrome": "npx cypress run --headed --browser  chrome",
    "test-firefox": "npx cypress run --headed --browser  firefox",
    "test-cucumber": "npx cypress run --browser chrome --spec cypress/integration/BDD/features/shoppingTest.feature --headed",
    "test-ts-examples": "npx cypress run --browser chrome --spec cypress/integration/*/*.ts --headed",
    "test-dashboard-samples": "npx cypress run --browser chrome --spec cypress/integration/POMExamples_ts/homepageTestsTS.ts --headless --record --key $record_key"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/integration/BDD/step_definitions/*.ts"
    ],
    "json": {
      "enabled": true,
      "output": "cypress/cucumberReports/results.json"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/honguyenit/cypress-demo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/honguyenit/cypress-demo/issues"
  },
  "homepage": "https://github.com/honguyenit/cypress-demo#readme",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.1.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.2",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "cypress": "^13.13.2",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "multiple-cucumber-html-reporter": "^3.7.0",
    "typescript": "^5.5.4"
  }
}
